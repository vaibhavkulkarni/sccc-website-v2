    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" type="text/css" href="{{asset "built/bootstrap.min.css"}}" />
    <link rel="stylesheet" type="text/css" href="{{asset "built/bootstrap-theme.min.css"}}" />
    <link rel="stylesheet" type="text/css" href="{{asset "built/style.css"}}" />
    <link rel="stylesheet" type="text/css" href="{{asset "built/camera.css"}}" />
